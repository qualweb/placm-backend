{
  "name": "placm-backend",
  "version": "1.0.0",
  "description": "Back-end to PLACM application",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "clean": "node clean",
    "build": "npm run clean && tsc --build",
    "start-dev": "ts-node-dev --respawn --transpileOnly ./index.ts",
    "start": "npm run build && NODE_ENV=production && node ./build/index",
    "start-win": "npm run build && set NODE_ENV=production && node ./build/index"
  },
  "keywords": [
    "placm",
    "website",
    "backend",
    "accessibility",
    "act"
  ],
  "author": "Bruno Andrade",
  "license": "ISC",
  "dependencies": {
    "@mysql/xdevapi": "8.0.19",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "5.3.1",
    "jsonld": "^2.0.2",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/jsonld": "^1.5.1",
    "@types/morgan": "^1.7.37",
    "@types/mysql": "^2.15.8",
    "@types/node": "^13.7.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.5"
  }
}
